conf t

 hostname "OV-R2"

 ipv6 unicast-routing

 interface Loopback64501
  description "loopback"
  ip address 10.0.3.2 255.255.255.255
  ip ospf authentication message-digest
  ip ospf message-digest-key 100 md5 ospfpassarea0
  ipv6 enable
  ipv6 address FD00:CCFF:200D::2/128
  ipv6 address FE80::2 link-local
  ipv6 ospf authentication ipsec spi 500 md5 1234567890abcdef1234567890abcdef
  ipv6 ospf 64501 area 0
  no shutdown
  exit

 interface Ethernet0/0
  description "to OV-R3"
  ip address 50.50.104.1 255.255.255.0
  ipv6 enable
  ipv6 address FE80::1 link-local
  ipv6 address 20FF:CCFF:200D:A1::1/64
  no shutdown
  exit 

 interface Ethernet0/1
  description "not used"
  no ip address
  shutdown
  exit

 interface Ethernet0/2
  description "to OV-R1"
  ip address 50.50.105.2 255.255.255.128
  ipv6 enable
  ipv6 address FE80::2 link-local
  ipv6 address 20FF:CCFF:200D:A2::2/64
  no shutdown
  exit 

 interface Ethernet0/3
  description "not used"
  no ip address
  shutdown
  exit

 key chain EIGRP64501
  key 1
   key-string eigrp64501secretpass
   exit
  exit

 router eigrp EIGRP64501
  address-family ipv4 unicast autonomous-system 1
   eigrp router-id 10.0.3.2
   network 50.50.104.0 0.0.0.255
   network 50.50.105.0 0.0.0.127
   network 10.0.3.2 0.0.0.0
   no shutdown
   af-interface default
    shutdown
    passive-interface
    exit-af-interface
   af-interface Ethernet0/0
    authentication key-chain EIGRP64501
    authentication mode md5
    no shutdown
    no passive-interface
    exit-af-interface
   af-interface Ethernet0/2
    authentication key-chain EIGRP64501
    authentication mode md5
    no shutdown
    no passive-interface
    exit-af-interface 
   af-interface Loopback64501
    authentication key-chain EIGRP64501
    authentication mode md5
    no shutdown
    passive-interface
    exit-af-interface
   topology base
    no auto-summary
    exit-af-topology
   exit-address-family
  address-family ipv6 unicast autonomous-system 1
   eigrp router-id 6.0.3.2
   no shutdown
   af-interface default
    shutdown
    passive-interface
    exit-af-interface
   af-interface Ethernet0/0
    authentication key-chain EIGRP64501
    authentication mode md5
    no shutdown
    no passive-interface
    exit-af-interface
   af-interface Ethernet0/2
    authentication key-chain EIGRP64501
    authentication mode md5
    no shutdown
    no passive-interface
    exit-af-interface  
   af-interface Loopback64501
    authentication key-chain EIGRP64501
    authentication mode md5
    no shutdown
    passive-interface
    exit-af-interface
   topology base
    exit-af-topology
   exit-address-family
  exit
 exit

 ip access-list standard 1
  remark "permit to NTP server access"
  10 permit 50.50.105.1 0.0.0.0
  20 permit 50.50.105.129 0.0.0.0
  500 deny any
  exit
 ntp access-group ipv4 peer 1

 ipv6 access-list NTPSERVER_IPV6_ACL
  remark "permit to NTP server access"
  permit ipv6 host 20FF:CCFF:200D:A2::1 any sequence 10
  permit ipv6 host 20FF:CCFF:200D:A3::1 any sequence 20
  deny ipv6 any any sequence 500
  exit
 ntp access-group ipv6 peer NTPSERVER_IPV6_ACL

 ntp authenticate
 ntp authentication-key 1 md5 ntpkey
 ntp trusted-key 1
 ntp update-calendar
 ntp server 50.50.105.1 prefer key 1
 ntp server 20FF:CCFF:200D:A2::1 prefer key 1
 ntp server 50.50.105.129 key 1
 ntp server 20FF:CCFF:200D:A3::1 key 1

 ip dns server
 ip domain-lookup
 ip domain name ov.com
 ip name-server 50.50.105.1 50.50.105.129 20FF:CCFF:200D:A2::1 20FF:CCFF:200D:A3::1

 ip route 0.0.0.0 0.0.0.0 50.50.105.1 1 name "to OV-R1"
 ipv6 route ::/0 20FF:CCFF:200D:A2::1 1 name "to OV-R1"

 ip dhcp excluded-address 0.0.0.0 50.50.104.2
 ip dhcp excluded-address 50.50.104.254 255.255.255.255

 service dhcp

 ip dhcp pool DHCP_POOL_1
  network 50.50.104.0 255.255.255.0
  default-router 50.50.105.1 50.50.105.129
  domain-name ov.com
  dns-server 50.50.104.1 50.50.104.2
  lease 8
  exit

 ipv6 dhcp pool DHCP_POOL_IPV6_1
  address prefix 20FF:CCFF:200D:A1::/64 lifetime 2073600 691200
  domain-name ov.com
  dns-server 20FF:CCFF:200D:A1::1
  exit

 interface Ethernet0/0
  ipv6 dhcp server DHCP_POOL_IPV6_1
  ipv6 nd managed-config-flag
  exit

 interface Ethernet0/2
  ip helper-address 50.50.105.1
  ipv6 dhcp relay destination FE80::1
  exit

 crypto key generate rsa usage-keys label SSH modulus 2048
 service password-encryption
 password encryption aes
 password logging
 
 aaa new-model
 aaa authentication login default local
 aaa authorization console
 aaa authorization exec default local
 
 username admin privilege 15 algorithm-type scrypt secret adminpassword
 username support privilege 1 algorithm-type scrypt secret supppass
 enable algorithm-type scrypt secret level 15 adminpassword
 enable algorithm-type scrypt secret level 1 supportassword
 enable algorithm-type scrypt secret level 2 editdesc
 privilege exec level 2 configure terminal
 privilege configure level 2 interface 
 privilege interface level 2 description

 line vty 0 4
  transport input ssh
  login authentication default
  logging synchronous
  exit
 line console 0
  logging synchronous
  exit
 logging on
 logging console 3
 logging buffered 3
 logging buffered 32000
 logging trap 6
 logging facility local1
 logging host 50.50.105.3
 logging host ipv6 20FF:CCFF:200D:A2::3
